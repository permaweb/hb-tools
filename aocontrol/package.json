	"scripts": {
		"dev:frontend": "NODE_ENV=development vite",
		"dev:backend": "nodemon --exec tsx server/index.ts",
		"dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
		"cli": "tsx server/cli.ts",
		"start:development": "NODE_ENV=development vite",
		"start:staging": "NODE_ENV=development vite",
		"start": "NODE_ENV=production tsx server/index.ts",
		"format": "eslint --fix . && npx prettier --write .",
		"test": "npm test",
		"test:verbose": "npm test -- --verbose",
		"test:coverage": "CI=true npm test -- --env=jsdom --coverage",
		"build": "NODE_ENV=production vite build",
		"deploy:main": "npm run build && permaweb-deploy --arns-name lunar",
		"deploy:staging": "npm run build && permaweb-deploy --arns-name lunar --undername staging"
	},
